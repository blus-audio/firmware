{
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "*.inc": "c",
        "type_traits": "c"
    },
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.dep": true,
        "**/build": true
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
    "makefile.configurations": [
        {
            "name": "blus_mini",
            "makeArgs": [
                "-j",
                "APP=blus_mini"
            ],
            "problemMatchers": [
                "$gcc"
            ],
        },
        {
            "name": "default",
            "makeArgs": [
                "-j",
            ],
            "problemMatchers": [
                "$gcc"
            ],
        }
    ],
}
